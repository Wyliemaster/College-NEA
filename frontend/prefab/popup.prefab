<div id="prefab-[[PREFAB_ID]]">
    <style>
        #prefab-popup-background {
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            position: fixed;

            background-color: rgba(0, 0, 0, 0.69);
        }

        .prefab-popup {
            color: white;
            transition: all 1s;
            transform-origin: 0 -25%;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            z-index: 10000 !important;
            display: flex;
            position: fixed;
            flex-direction: column;
            align-items: center;

            min-width: 90%;
            min-height: 80%;

            max-width: 95%;
            max-height: 90%;

            background-color: rgb(75, 75, 75);
            border: 2px groove black;

            justify-content: space-evenly;

        }

        #prefab-popup-container {
            width: inherit;
            display: flex;
            flex-direction: row;
            justify-content: space-evenly;
            flex-grow: 1;
        }

        #prefab-popup-title {
            font-size: xx-large;
            flex-grow: 1;
        }

        #prefab-popup-description {
            font-size: large;
            flex-grow: 1;
        }
    </style>
    <div id="prefab-popup-background"></div>
    <div class="prefab-popup">
        <p id="prefab-popup-title">[[POPUP_TITLE]]</p>
        <p id="prefab-popup-description">[[POPUP_DESC]]</p>
        <div id="prefab-popup-container">[[POPUP_CONTAINER]]</div>
    </div>
</div>
</div>